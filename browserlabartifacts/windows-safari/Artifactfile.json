{
  "$schema": "https://raw.githubusercontent.com/Azure/azure-devtestlab/master/schemas/2015-01-01/dtlArtifacts.json",
  "title": "Safari",
  "description": "Installs Safari (next to all other common browser using chocolatey allbrowsers)",
  "tags": [
    "Windows"
  ],
  "iconUri": "https://chocolatey.org/Content/Images/packageDefaultIcon.png",
  "targetOsType": "Windows",
  "runCommand": {
    "commandToExecute": "powershell.exe -ExecutionPolicy bypass -File startChocolatey.ps1 -PackageList allbrowsers"
  }
}